@model List<XoneHR.Models.TblCheckListTypes>
@{
    ViewBag.Title = "Create";
    var i = 0;
}

<script src="~/plugins/Toast/src/jquery.toast.js"></script>
<link href="~/plugins/Toast/src/jquery.toast.css" rel="stylesheet" />

<script src="~/plugins/Maskjs/apps.min.js"></script>
<script src="~/plugins/Maskjs/form-input-mask.min.js"></script>
<script src="~/plugins/Maskjs/jquery.inputmask.bundle.min.js"></script>

<script type="text/javascript">
    $(document).ready(function () {

        $("#lengthchk").hide();      
        DeclarationtabContentList();
        GetDocumentType();
        GetRace();
        GetReligion();
        GetMariatalStatus();
        GetCitizenship();
        GetSKillSet();
        GetLangKnown();
        GetEmptype();
        GetDesignation();
        $("#Resident_Id").append('<option value="0">--Select--</option>').selectpicker('refresh');
        //$("#Dep_UserType").change(function () {
        //    GetDesignation($(this).val());
        //});
        $(".otherreligiontab").hide();
        $(".otherracetab").hide();
        $(".otherrtab").hide();

        if ($("#Cand_Emp").val() == 1) {
            $("#employee").show();
            $("#Candidate").hide();
            $("#EmpTab").show();
            $(".Cand_Salexp").hide();
        }
        else {
            $("#employee").hide();
            $("#Candidate").show();
            $("#EmpTab").hide();
            $(".Cand_Salexp").show();
        }
       $("#CheckListTab").hide();
        $("#LeaveTab").hide();

        $("#ddlDocumentSubtypeID").append('<option value="0">Sub Type</option>').selectpicker('refresh');
        $("#ddlDocumentTypeID").change(function () {

            var DocumenttypeID = $(this).val();

            GetDocumentSubType(DocumenttypeID);
        });

        $("#BtnAddCandprofile").click(function (event) {

            $.validator.addMethod("custom_number", function (value, element) {
                return this.optional(element) || value === "NA" ||
                    value.match(/^[0-9,\+-]+$/);
            }, "Please enter a valid number");
            $.validator.addMethod("check_item_dropdown", function (value, element) {
                return this.optional(element) || value != 0;
            }, "Please select an item from the dropdown list.");

            var flag = 0;
            if ($("#Resident_Id").val() == 1 || $("#Resident_Id").val() == 2)
            {
                flag = 4; //NRIC Number exist
            }
            else {
                flag = 5; //Fin Num
            }
            $.ajax({
                data: { Mob: $("#CandMobile").val(), flag: flag },
                url: "/Candidate/Mobile_Email_Validation",
                async: false
            }).done(function (data) {

                if (data == 1) {
                    {
                        if (flag == 4)
                        { $("#nric_unique").show(); $("#FinNo").hide(); }
                        else
                        { $("#FinNo").show(); $("#nric_unique").hide(); }
                    }
                }
                else {
                    //initilize validation plugin on this form.
                    $("#CandidateProfile").validate({
                        rules: {
                            CandMobile: {
                                //  required: true,
                                custom_number: true,
                                minlength: 8,
                                maxlength: 15
                            },
                            CandPhone: {
                                // required: true,
                                custom_number: true,
                                minlength: 8,
                                maxlength: 15
                            },
                            otherrace: {
                                required: true,
                            },
                            otherreligion: {
                                required: true,
                            },
                            CandDob: {
                                required: true,
                                //  date: true
                            },
                            CandRegDate: {
                                required: true,
                                // date: true,
                            },
                            CandSalrexpted: {
                                //  required: true,
                                number: true
                            },
                            CizenID: {
                                check_item_dropdown: true,
                                required: true,
                            },
                            MaritID: {
                                check_item_dropdown: true,
                                required: true,
                            },
                            ReligID: {
                                check_item_dropdown: true,
                                required: true,
                            },
                            RaceID: {
                                check_item_dropdown: true,
                                required: true,
                            },
                            DesigID: {
                                check_item_dropdown: true,
                                required: true,
                            },
                            GradeId: {
                                check_item_dropdown: true,
                                required: true,
                            },
                            CandEmail: {
                                //  required:true
                            },
                            //Citizen details
                            //CitzOrdrDate: {
                            //    required: true
                            //}
                        }

                    });
                    //run validation process
                    if ($('#CandidateProfile').valid()) {

                        var RaceStatus = 0; var RelegionStatus = 0;

                        if ($("#RaceID").val() == -1) {
                            $.ajax({
                                url: "/Candidate/RaceCreate",
                                data: { RaceName: $("#otherrace").val() },
                                async: false
                            }).done(function (data) {
                                if (data.Result > 0) { $("#Race_name").val(data.Result); }
                                else if (data.Result = -1) {
                                    $.toast({
                                        text: "Race Name Already Exists.Please Select Same From Race List...!",
                                        position: 'top-right',
                                        hideAfter: 2000,
                                        showHideTransition: 'slide',
                                        loader: false
                                    }); RaceStatus = 1;
                                }
                                else {
                                    $.toast({
                                        text: "Race Name Save Failed",
                                        position: 'top-right',
                                        hideAfter: 2000,
                                        showHideTransition: 'slide',
                                        loader: false
                                    }); RaceStatus = 1;;
                                }
                            });
                        }

                        if ($("#ReiligionID").val() == -1) {
                            $.ajax({
                                url: "/Candidate/ReligionCreate",
                                data: { ReligionName: $("#otherreligion").val() },
                                async: false
                            }).done(function (data) {
                                if (data.Result > 0) { $("#Religion_name").val(data.Result); }
                                else if (data.Result == -1) {
                                    $.toast({
                                        text: "Religion Name Already Exists.Please Select Same From Race List...!",
                                        position: 'top-right',
                                        hideAfter: 2000,
                                        showHideTransition: 'slide',
                                        loader: false
                                    }); RelegionStatus = 1;
                                }
                                else {
                                    $.toast({
                                        text: "Religion Name Save Failed",
                                        position: 'top-right',
                                        hideAfter: 2000,
                                        showHideTransition: 'slide',
                                        loader: false
                                    }); RelegionStatus = 1;
                                }
                            });
                        }
                        
                        if (RelegionStatus == 0 && RaceStatus == 0) //Race,Relegion Add Validation
                        {
                            $.ajax({

                                type: "POST",
                                url: "/Candidate/AddNewCandidate",
                                data: $("#CandidateProfile").serialize(),
                                async: false
                            }).done(function (data) {

                                if (data == true) {
                                    $.ajax({
                                        type: "POST",
                                        url: "/Candidate/AddNewCitizenDetails",
                                        data: $("#CandidateProfile").serialize(),
                                        async: false
                                    }).done(function (data) {
                                        if (data) {
                                            $.toast({

                                                text: "Saved Successfully",
                                                position: 'top-right',
                                                hideAfter: 3000,
                                                showHideTransition: 'slide',
                                                loader: false,
                                            })
                                        }
                                        else {
                                            $.toast({
                                                text: "Successfully Saved",  //, Citizen Details Cann't Add!
                                                position: 'top-right',
                                                hideAfter: 3000,
                                                showHideTransition: 'slide',
                                                loader: false,
                                            })
                                        }
                                    });
                                    //$('.nav-tabs > .active').next('li').find('a').trigger('click');
                                }
                                else {
                                    $.toast({
                                        text: "Application Error Please Contact Administrator!",
                                        position: 'top-right',
                                        hideAfter: 3000,
                                        showHideTransition: 'slide',
                                        loader: false,
                                    })
                                }
                            });
                        }                                                
                    }
                }

            });

        });

        $("#BtnCitizenDetails").click(function () {

            $("#Candidatecitizendetails").validate({

                rules: {
                    CitzOrdrDate: {
                        required: true,
                        // date: true,
                    },
                }
            });
            if ($("#Candidatecitizendetails").valid()) {
                $.ajax({

                    type: "POST",
                    url: "/Candidate/AddNewCitizenDetails",
                    data: $("#Candidatecitizendetails").serialize()

                }).done(function (data) {

                    if (data) {
                        $.toast({

                            text: "Saved Successfully",
                            position: 'top-right',
                            hideAfter: 3000,
                            showHideTransition: 'slide',
                            loader: false,
                        })

                        //$('.nav-tabs > .active').next('li').find('a').trigger('click');

                    }
                    else {
                        $.toast({

                            text: "Application Error Please Contact Administrator!",
                            position: 'top-right',
                            hideAfter: 3000,
                            showHideTransition: 'slide',
                            loader: false,
                        })
                    }

                })
            }
        });

        $("#btnAddEmployer").click(function () {

            $("#CandidateEmpDetails").validate({
                rules: {
                    EmphSalryLast: {
                        required: true,
                        number: true
                    }
                }
            });
            if ($("#CandidateEmpDetails").valid()) {
                $.ajax({

                    type: "POST",
                    url: "/Candidate/AddNewEmployerDetails",
                    data: $("#CandidateEmpDetails").serialize()

                }).done(function (data) {

                    if (data > 0) {
                        $.toast({

                            text: "Saved Successfully",
                            position: 'top-right',
                            hideAfter: 3000,
                            showHideTransition: 'slide',
                            loader: false,
                        })

                        LoadEmpHistory(data);

                        $("#EmphEmployer").val("");
                        $("#empStartenddate").val("");
                        $("#EmphSalryLast").val("");

                    }
                    else {
                        $.toast({

                            text: "Application Error Please Contact Administrator!",
                            position: 'top-right',
                            hideAfter: 3000,
                            showHideTransition: 'slide',
                            loader: false,
                        })
                    }

                })
            }

        });

        $("#BtnAddCandidateBank").click(function () {

            $("#CandidateBankDetails").validate();
            if ($("#CandidateBankDetails").valid()) {

                $.ajax({

                    type: "POST",
                    url: "/Candidate/AddNewBankDetails",
                    data: $("#CandidateBankDetails").serialize()

                }).done(function (data) {

                    if (data) {
                        if (data.Result > 0) {
                            $.toast({

                                text: "Bank A/c Number Already Exists",
                                position: 'top-right',
                                hideAfter: 3000,
                                showHideTransition: 'slide',
                                loader: false,
                            })
                        }
                        else {
                            $.toast({

                                text: "Bank Details Saved Successfully",
                                position: 'top-right',
                                hideAfter: 3000,
                                showHideTransition: 'slide',
                                loader: false,
                            })
                        }

                        //$('.nav-tabs > .active').next('li').find('a').trigger('click');

                    }
                    else {
                        $.toast({

                            text: "Application Error Please Contact Administrator!",
                            position: 'top-right',
                            hideAfter: 3000,
                            showHideTransition: 'slide',
                            loader: false,
                        })
                    }

                })
            }

        });

        $("#btnuploadfiles").click(function () {

            $.validator.addMethod("check_item_dropdown", function (value, element) {
                return this.optional(element) || value != 0;
            }, "Please select an item from the dropdown list.");
            $("#UploadDocuments").validate({
                rules: {
                    ddlDocumentTypeID: {
                        check_item_dropdown: true,
                        required: true,
                    },
                    ddlDocumentSubtypeID: {
                        check_item_dropdown: true,
                        required: true,
                    }
                }
            })

            var formData = new FormData();

            var totalFiles = document.getElementById("newdoc").files.length;
            if (totalFiles > 0) {

                for (var i = 0; i < totalFiles; i++) {
                    var file = document.getElementById("newdoc").files[i];

                    formData.append("FileUpload", file);
                    formData.append("DocumentTypeID", $("#ddlDocumentTypeID").val());
                    formData.append("DocumentsubTypeID", $("#ddlDocumentSubtypeID").val());
                    formData.append("HidTab2", $("#HidTab2").val());
                }
                if ($("#UploadDocuments").valid()) {
                    $.ajax({

                        type: "POST",
                        url: "/Candidate/UploadNewDocuments",
                        data: formData,
                        dataType: 'json',
                        contentType: false,
                        processData: false

                    }).done(function (data) {

                        if (data) {
                            $.toast({

                                text: "Saved Successfully",
                                position: 'top-right',
                                hideAfter: 3000,
                                showHideTransition: 'slide',
                                loader: false,
                            })

                            LoadDocuments(0);

                        }
                        else {
                            $.toast({

                                text: "Application Error Please Contact Administrator!",
                                position: 'top-right',
                                hideAfter: 3000,
                                showHideTransition: 'slide',
                                loader: false,
                            })
                        }

                    });
                }

            }
            else {
                $.toast({

                    text: "Please Upload File",
                    position: 'top-right',
                    hideAfter: 3000,
                    showHideTransition: 'slide',
                    loader: false,
                })

            }
        })

        $("#BtnDocusavecontinue").click(function () {

            //$('.nav-tabs > .active').next('li').find('a').trigger('click');
        });

        $("#BtnemphContinue").click(function () {

            //$('.nav-tabs > .active').next('li').find('a').trigger('click');
        });

        $("#div_funds").hide();
        $("#div_levy").hide();
       
        $(".tr_wp").hide();
        $("#tr_citizen").hide();
        $("#Citizentab").hide();
        GetFundTypes();

        $("#Resident_Id").change(function () {
            if ($(this).val() == 1) //Citizen
            {
                $("#tr_citizen").show();
               // $("#Citizentab").show();
                $(".tr_wp").hide();
                $("#div_levy").hide();
                $("#div_funds").show();
            }
            else if ($(this).val() == 2) {
                $("#tr_citizen").show();
               // $("#Citizentab").hide();
                $(".tr_wp").hide();
                $("#div_levy").hide();
                $("#div_funds").show();
            }
            else if ($(this).val() == 3) {
                $(".tr_wp").show();
                $("#tr_citizen").hide();
              //  $("#Citizentab").hide();
                $("#div_levy").show();
                $("#div_funds").hide();
            }
            else {
                $(".tr_wp").show();
                $("#tr_citizen").hide();
               // $("#Citizentab").hide();
                $("#div_levy").hide();
                $("#div_funds").hide();
            }
        });

        //Photo Upload btton Hide in Form load
        $(".Uploadbutton").hide();

        $(".uploadphoto").click(function () {

            var Checkvalue = $(this).val();

            if (Checkvalue == 1) {

                $(".Filetype").hide();
                $(".Uploadbutton").show();
                $("#Doctexts").html("Upload Photo");
            }
            else {

                $(".Filetype").show();
                $(".Uploadbutton").hide();
                $("#Doctexts").html("Upload Documents");
            }

        });

        $("#Btnphotoupload").click(function () {

            var formData = new FormData();

            var totalFiles = document.getElementById("newdoc").files.length;

            if (totalFiles > 0) {

                for (var i = 0; i < totalFiles; i++) {
                    var file = document.getElementById("newdoc").files[i];

                    formData.append("FileUpload", file);

                }

                $.ajax({

                    type: "POST",
                    url: "/Candidate/UploadPhoto",
                    data: formData,
                    dataType: 'json',
                    contentType: false,
                    processData: false

                }).done(function (data) {

                    if (data) {
                        $.toast({

                            text: "Saved Successfully",
                            position: 'top-right',
                            hideAfter: 3000,
                            showHideTransition: 'slide',
                            loader: false,
                        })

                    }
                    else {
                        $.toast({

                            text: "Application Error Please Contact Administrator!",
                            position: 'top-right',
                            hideAfter: 3000,
                            showHideTransition: 'slide',
                            loader: false,
                        })
                    }

                });
            }
            else {
                $.toast({

                    text: "Please Upload File",
                    position: 'top-right',
                    hideAfter: 3000,
                    showHideTransition: 'slide',
                    loader: false,
                })
            }

        })

        $("#BtnEmpSave").click(function () {

            $.validator.addMethod("check_item_dropdown", function (value, element) {
                return this.optional(element) || value != 0;
            }, "Please select an item from the dropdown list.");
            $.validator.addMethod("check_offday_dropdown", function (value, element) {
                return this.optional(element) || value != -1;
            }, "Please select an item from the dropdown list.");

            $("#employeedatadetails").validate({
                rules: {
                    StartDate: {
                        required: true,
                        //  date: true
                    },
                    //EndDate: {
                    //    required: true,
                    //    date: true
                    //},
                    Employeetyp: {
                        required: true,
                        check_item_dropdown: true
                    },
                    EmpSubTypeID: {
                        required: true,
                        check_item_dropdown: true
                    },
                    EmpRegNo: {
                        required: true
                    },
                    //Emp_SetOffDay: {
                    //    required: true,
                    //    check_offday_dropdown:true
                    //},
                    RestDay_Fixed: {
                        required: true,
                        check_offday_dropdown: true
                    }
                    //RestDay_Optional: {
                    //    required: true,
                    //    check_offday_dropdown:true
                    //}
                }
            });
            if ($("#employeedatadetails").valid()) {

                $.ajax({

                    type: "POST",
                    url: "/Candidate/AddNewEmployee",
                    data: $("#employeedatadetails").serialize()
                }).done(function (data) {
                    var id = data.CanId;
                    $("#Cnd_id").val(id);
                    if (data.Id == 1) {
                        $.toast({
                            text: "Saved Successfully",
                            position: 'top-right',
                            hideAfter: 3000,
                            showHideTransition: 'slide',
                            loader: false,
                        })
                        // window.location.href = "/Employee/Index";
                        $("#CheckListTab").show();
                        //Leave tab
                        $("#LeaveTab").show();
                       $('.nav-tabs > .active').next('li').find('a').trigger('click');
                        $.ajax({
                            url: "/Candidate/EmployeeLeaves",
                            data: { EmployeeId: 0 }
                        }).done(function (data) {
                            $("#Leavedetail").html(data);
                        });
                    }
                    else if (data.Id == 2) {
                        $.toast({
                            text: "Employee Code Already Registered",
                            position: 'top-right',
                            hideAfter: 3000,
                            showHideTransition: 'slide',
                            loader: false,
                        })
                    }
                    else {
                        $.toast({
                            text: "Application Error Please Contact Administrator!",
                            position: 'top-right',
                            hideAfter: 3000,
                            showHideTransition: 'slide',
                            loader: false,
                        })
                    }

                });
            }

        });

        $('#MaritalStatusID').change(function () {
            if ($(this).val() != 0) {
                $("label[for='MaritalStatusID']").text('');
            }
            else {
                $("label[for='MaritalStatusID']").text('Please select an item from the dropdown list.');
            }

        })
      
        $('#CitizenID').change(function () {
            GetResidentStatus($(this).val());
            if ($(this).val() != 0) {
                $("label[for='CitizenID']").text('');
                if($(this).val() == 2)
                {
                    $(".Pnumber").hide();                    
                }
                else {
                    $(".Pnumber").show();
                }

                if ($("#CandGender").val() == 1 && $(this).val() == 2)  //Sigapore citizen mails only show NS details..
                    $("#Citizentab").show();                
                else
                    $("#Citizentab").hide();
            }
            else {
                $("label[for='CitizenID']").text('Please select an item from the dropdown list.');
            }
            
        })
        $('#ReiligionID').change(function () {
            $("#Religion_name").val($('#ReiligionID').val());

            if ($(this).val() != 0) {
                $("label[for='ReiligionID']").text('');
                if ($(this).val() == -1) {
                    // $("#AddReligionModal").modal("show");
                    $(".otherreligiontab").show();
                    $(".otherrtab").hide();
                }
                else { $(".otherreligiontab").hide(); $(".otherrtab").show(); }
            }
            else {
                $("label[for='ReiligionID']").text('Please select an item from the dropdown list.');
                $(".otherreligiontab").hide();
            }
        });
        $('#RaceID').change(function () {
            $("#Race_name").val($("#RaceID").val());

            if ($(this).val() != 0) {
                $("label[for='RaceID']").text('');
                if($(this).val() == -1)
                {
                    // $("#AddRaceModal").modal("show");
                    $(".otherracetab").show();
                    if($('#ReiligionID').val() != -1)
                        $(".otherrtab").show();
                    else
                        $(".otherrtab").hide();
                }
                else
                { $(".otherracetab").hide(); }
            }
            else {
                $("label[for='RaceID']").text('Please select an item from the dropdown list.');
                $(".otherracetab").hide();
            }

        })

        $("#CandGender").change(function () {
            if ($('#CitizenID').val() == 2 && $(this).val() == 1) {
                $("#Citizentab").show();
            }
            else
            {
                $("#Citizentab").hide();
            }
        })

        $("#BtnAddReligion").click(function () {
            $.ajax({

                type: "POST",
                url: "/Candidate/ReligionCreate",
                data: { ReligionName: $("#ReligionName").val() }
            }).done(function (data) {
                
                ToastCommon(data.Message, data.Icon);
                $("#AddReligionModal").modal("hide");
                GetReligion();
                $("#ReiligionID").val(data.Result);
                $("#ReiligionID").selectpicker('val', data.Result);
            });
        })

        $("#BtnAddRace").click(function () {
            $.ajax({

                type: "POST",
                url: "/Candidate/RaceCreate",
                data: { RaceName: $("#Racename").val() }
            }).done(function (data) {
                ToastCommon(data.Message, data.Icon);
                $("#AddRaceModal").modal("hide");
                GetRace();
                $("#RaceID").val(data.Result);
                $("#RaceID").selectpicker('val', data.Result);
            });
        })

        $('#ddlDocumentTypeID').change(function () {
            if ($(this).val() != -1) {
                $("label[for='ddlDocumentTypeID']").text('');
            }
            else {
                $("label[for='ddlDocumentTypeID']").text('Please select an item from the dropdown list.');
            }

            if ($(this).val() == 0) {
                $(".Filetype").hide();
                $(".Uploadbutton").show();
                $("#Doctexts").html("Upload Photo");
            }
            else {
                $(".Filetype").show();
                $(".Uploadbutton").hide();
                $("#Doctexts").html("Upload Documents");
            }

        })
        $('#ddlDocumentSubtypeID').change(function () {
            if ($(this).val() != 0) {
                $("label[for='ddlDocumentSubtypeID']").text('');
            }
            else {
                $("label[for='ddlDocumentSubtypeID']").text('Please select an item from the dropdown list.');
            }

        })

        $('#DesignationID').change(function () {
            if ($(this).val() != 0) {
                $("label[for='DesignationID']").text('');

                var ddlBox = $("#GradeId");

                $.ajax({
                    url: "/Candidate/GetGrade",
                    data: { desigid: $('#DesignationID').val() },
                    async: false
                }).done(function (data) {

                    ddlBox.empty();
                    ddlBox.append('<option value="0">--Select--</option>').selectpicker('refresh');

                    $.each(data, function (i, item) {

                        ddlBox.append('<option value=' + item.GradeID + '>' + item.Gradename + '</option>').selectpicker('refresh');
                    });

                });

            }
            else {
                $("label[for='DesignationID']").text('Please select an item from the dropdown list.');
            }

        });

        $("#GradeId").change(function () {
            if ($(this).val() != 0) {
                $("label[for='GradeId']").text('');
            }
            else {
                $("label[for='GradeId']").text('Please select an item from the dropdown list.');
            }
        });

        $('#CandRegDate').change(function () {
            $(this).blur();
        })

        $("#mask_date").change(function () {
            $(this).blur(); 
        })

        $("#ddlSkillSet").change(function () {

            if ($(this).val() != null) {
                $("label[for='ddlSkillSet']").text('');
            }
            else {
                $("label[for='ddlSkillSet']").text('This field is required');
            }

        })
        $("#ddlLangKnown").change(function () {

            if ($(this).val() != null) {
                $("label[for='ddlLangKnown']").text('');
            }
            else {
                $("label[for='ddlLangKnown']").text('This field is required');
            }

        })
        $('#CitzOrdrDate').change(function () {
            $(this).blur();
        })

        $("#mob_unique").hide();
        $("#mail_unique").hide();
        $("#phone_unique").hide();

        //$("#CandMobile").focusout(function () {

        //    $.ajax({
