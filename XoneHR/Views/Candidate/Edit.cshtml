    function GetEmployeeLastTab() {
        $.ajax({

            type: "POST",
            url: "/Candidate/GetEditEmployeeEmpDetails",
            data: { CandID: $("#CandID").val() },
            async: false
        }).done(function (data) {

            if (data == 1) {
                GetEmptype(0);
                GetFundTypes(0);
            } else {

                GetEmptype(data.EmpTypID);
                GetFundTypes(data.FundType);
                $("#StartDate").val(moment(data.EmpStartDate).format($("#HidDateFormat").val()));   //'MM/DD/YYYY'
                if (moment(data.EmpEndDate).format($("#HidDateFormat").val()) != '01/01/0001') {    //'MM/DD/YYYY'
                    $("#EndDate").val(moment(data.EmpEndDate).format($("#HidDateFormat").val()));   //'MM/DD/YYYY'
                }
                else
                    $("#EndDate").val();

                $("#BasicSalary").val(data.BasicSalary);
                $("#Levy").val(data.EmpLevyAmnt);
                $("#HidEmpID").val(data.EmpID);
                $("#EmployeeID").val(data.EmpID);
                // $("#HidSalaEmpID").val(data.SalaEmpID);
                $("#HidEmpPartTime_Pay").val(data.EmpPartTime_Pay);
                $("#EmpRegNo").val(data.EmpRegNo);

                var rad = data.CPF_Status; 
                if (rad == true) {
                    var $radios = $('input:radio[name=CPF]');
                    //if ($radios.is(':checked') === false) {
                    //    $radios.filter('[value=1]').prop('checked', true);
                    //}
                    $radios.filter('[value=1]').prop('checked', true);
                } else {
                    var $radios = $('input:radio[name=CPF]');
                    //if ($radios.is(':checked') === false) {
                    //    $radios.filter('[value=0]').prop('checked', true);
                    //}
                    $radios.filter('[value=0]').prop('checked', true);
                }

                $("#Emp_PermenanentType").val(data.Emp_PermenanentType);

                if ($("#HidEmpPartTime_Pay").val() == 1) {
                    $("#BSal_label").html("Hourly Base Salary");
                    $("#rd_PayStruct1").attr("checked", "checked");
                    $("#DivPartemp").show();
                }
                else if ($("#HidEmpPartTime_Pay").val() == 2) {
                    $("#BSal_label").html("Daily Base Salary");
                    $("#rd_PayStruct1").attr("checked", "checked");
                    $("#DivPartemp").show();
                }
                else {
                    $("#BSal_label").html("Basic Salary");
                    $("#DivPartemp").hide();
                }
            }
        })
    }

