    function GetEditCitizenDetails(Type) {
        $.ajax({

            type: "POST",
            url: "/Candidate/GetEditCitizenDetails",
            data: { CandID: $("#CandID").val() }

        }).done(function (data) {
            if (Type == 1) {

                $("#CitzDetID").val(data.CitzDetID);
                $("#CitzDetCandID").val(data.CandID);
                $("#CitzNsVaction").val(data.CitzNsVaction);
                $("#CitzOrdrDate").val(moment(data.CitzOrdrDate).format($("#HidDateFormat").val()));    //'MM/DD/YYYY'
                $("#CitzReglrVaction").val(data.CitzReglrVaction);
                $("#CitzOrdrDateOne").val(moment(data.CitzOrdrDateOne).format($("#HidDateFormat").val()));   //'MM/DD/YYYY'

                var rad = data.CitzReservist;
                if (rad == true) {
                    var $radios = $('input:radio[name=CitzReservist]');
                    if ($radios.is(':checked') === false) {
                        $radios.filter('[value=1]').prop('checked', true);
                    }
                } else {
                    var $radios = $('input:radio[name=CitzReservist]');
                    if ($radios.is(':checked') === false) {
                        $radios.filter('[value=0]').prop('checked', true);
                    }
                }

            }
            else {

                $("#CitzNsVaction").val("");
                $("#CitzOrdrDate").val("");
                $("#CitzReglrVaction").val("");
                $("#CitzOrdrDateOne").val("");
                var $radios = $('input:radio[name=CitzReservist]');
                if ($radios.is(':checked') === false) {
                    $radios.filter('[value=0]').prop('checked', true);
                }
            }
        })
    }

