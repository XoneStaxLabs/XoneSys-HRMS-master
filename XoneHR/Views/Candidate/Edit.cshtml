                url: "/Candidate/RemoveDocuments",
                data: { DocID: $(this).attr("data-docid") }
            }).done(function (data) {
                LoadDocuments(0);
            });

            document.getElementById("newdoc").innerHTML = docfiles;
            $('select[name=ddlDocumentTypeID]').val(doctypname);
            $('select[name=ddlDocumentTypeID]').selectpicker('refresh');
            $('select[name=ddlDocumentSubtypeID]').val(docstypname);
            $('select[name=ddlDocumentSubtypeID]').selectpicker('refresh');

        });

        $("#BtnEmpSave").click(function () {

            $.validator.addMethod("check_item_dropdown", function (value, element) {
                return this.optional(element) || value != 0;
            }, "Please select an item from the dropdown list.");

            $("#employeedatadetails").validate({
                rules: {
                    StartDate: {
                        required: true,
                        //date: true
                    },
                    Employeetyp: {
                        required: true,
                        check_item_dropdown: true
                    },
                    Emp_PermenanentType: {
                        required: true
                    },
                    EmpRegNo: {
                        required: true
                    }
                }
            });
            if ($("#employeedatadetails").valid()) {

                $.ajax({

                    type: "POST",
                    url: "/Candidate/EditNewEmployee",
                    data: $("#employeedatadetails").serialize()

                }).done(function (data) {

                    if (data.Id == 1) {
                        $.toast({

                            text: "Updated Successfully",
                            position: 'top-right',
                            hideAfter: 3000,
                            showHideTransition: 'slide',
                            loader: false,
                        })

                        // window.location.href = "/Employee/Index";
                    }
                    else if (data.Id == 2) {
                        $.toast({

                            text: "Employee Code Already Registered",
                            position: 'top-right',
                            hideAfter: 3000,
                            showHideTransition: 'slide',
                            loader: false,
                        })
                    }
                    else {
                        $.toast({

                            text: "Application Error Please Contact Administrator!",
                            position: 'top-right',
                            hideAfter: 3000,
                            showHideTransition: 'slide',
                            loader: false,
                        })
                    }

                });
            }

        });

        $("#DivEmpEnd").hide();
        $("#BSal_label").html("Basic Salary");
        $("#div_Emp_PermenanentType").hide();
        $("#Employeetyp").change(function () {

            if ($(this).val() == 1) {
                $("#DivEmpEnd").hide();
                $("#DivPartemp").hide();
                $("#BSal_label").html("Basic Salary");
                $("#div_Emp_PermenanentType").show();
            }
            else if ($(this).val() == 2) {
                $("#DivEmpEnd").hide();
                $("#DivPartemp").show();
                $("#div_Emp_PermenanentType").hide();
            }
            else {
                $("#DivEmpEnd").show();
                $("#DivPartemp").hide();
                $("#BSal_label").html("Basic Salary");
                $("#div_Emp_PermenanentType").hide();
            }

        });

        $("#BtnAddPLRD").click(function () {
            $("#PLRD_Details").validate();
            if ($("#PLRD_Details").valid()) {

                $.ajax({
                    type: "POST",
                    url: "/Candidate/AddPLRDDetails",
                    data: $("#PLRD_Details").serialize()
                }).done(function (data) {

                    if (data) {
                        $.toast({

                            text: "Saved Successfully",
