@model IEnumerable<XoneHR.Models.EmployeeValidationDocs>
@{
    ViewBag.Title = "ValidDocumentResults";
    var i = 0;
    Layout = null;
}



<table  class="table table-bordered table-hover">
    <thead>
        <tr>

           
            <th style="width:15%;">No</th>
            <th>Document Type</th>
           

        </tr>
    </thead>
    <tbody>


        @foreach (var items in Model)
        {
            i=i+1;
            <tr>

                <td>@i</td>
                <td>@items.DocStypName</td>

            </tr>
             
        }


    </tbody>
</table>
