<body class="hold-transition skin-blue sidebar-mini">
    <input type="hidden" value="@XoneHR.Models.SessionManage.Current.Dateformat" id="HidDateFormat" />
    <div class="wrapper">

        <header class="main-header">

            <!-- Logo -->
            <a href="/DashBoard/DashBoard" class="logo">
                <!-- mini logo for sidebar mini 50x50 pixels -->
                <span class="logo-mini"><b>A</b>LT</span>
                <!-- logo for regular state and mobile devices -->
                <span class="logo-lg"><img src="/images/logo.png" alt="" /></span>
            </a>

            <!-- Header Navbar: style can be found in header.less -->
            <nav class="navbar navbar-static-top" role="navigation">
                <!-- Sidebar toggle button-->
                <a href="#" class="sidebar-toggle" data-toggle="offcanvas" role="button">
                    <span class="sr-only">Toggle navigation</span>
                </a>
                <span class="stax">DRAGNET SECURITY</span>
                <!-- Navbar Right Menu -->
                <div class="navbar-custom-menu">
                    <ul class="nav navbar-nav">

                        <li class="dropdown notifications-menu" id="LeaveNotification"></li>
                        <li class="dropdown notifications-menu" id="PassportNotification"></li>
                        <li class="dropdown notifications-menu" id="PLRDNotification"></li>
                        
                        <li class="dropdown user user-menu">
                            <a href="#" class="dropdown-toggle" data-toggle="dropdown">
                                <img src="@XoneHR.Models.SessionManage.Current.AppPhoto" class="user-image" alt="User Image">
                                <span class="hidden-xs">@XoneHR.Models.SessionManage.Current.AppUname</span>
                            </a>
                            <ul class="dropdown-menu">
                                <!-- User image -->
                                <li class="user-header">
                                    <img src="@XoneHR.Models.SessionManage.Current.AppPhoto" class="img-circle" alt="User Image">
                                    <p>
                                        @(XoneHR.Models.SessionManage.Current.AppUname +" - "+XoneHR.Models.SessionManage.Current.Designame)
                                        <small>Member since Nov. 2012</small>
                                    </p>
                                </li>
                                <!-- Menu Body -->
                                @*<li class="user-body">
                                    <div class="col-xs-4 text-center">
                                        <a href="#">Password</a>
                                    </div>
                                   
                                </li>*@
                                <!-- Menu Footer-->
                                <li class="user-footer">
                                    <div class="pull-left">
                                        <a href="/ApplicationUser/UserProfile?AppUID=@XoneHR.Models.SessionManage.Current.AppUID" class="btn btn-default btn-flat">Profile</a>
                                    </div>
                                    <div class="pull-left">
                                        <a href="/Login/UserPasswordBox" class="btn btn-default btn-flat">Settings</a>
                                    </div>
                                    <div class="pull-right">
                                        <a href="#" class="btn btn-default btn-flat" id="SignOut">Sign out</a>
                                    </div>
                                </li>
                            </ul>
                        </li>


                    </ul>
                </div>

            </nav>
        </header>
        <!-- Left side column. contains the logo and sidebar -->
        <aside class="main-sidebar">
            <!-- sidebar: style can be found in sidebar.less -->
            <section class="sidebar">
                <!-- Sidebar user panel -->
                <div class="user-panel">
                    <div class="pull-left image">
                        <img src="@XoneHR.Models.SessionManage.Current.AppPhoto" class="img-circle" alt="User Image">
                        <span>Admin</span>
                    </div>
                    <div class="pull-left info">
                        <p></p>
                        <a href="#"><i class="fa fa-circle text-success"></i> (Online)</a>
                    </div>
                </div>
                <!-- search form -->
                @*<form action="#" method="get" class="sidebar-form">
                    <div class="input-group">
                        <input type="text" name="q" class="form-control" placeholder="Search...">
                        <span class="input-group-btn">
                            <button type="submit" name="search" id="search-btn" class="btn btn-flat"><i class="fa fa-search"></i></button>
                        </span>
                    </div>
                </form>*@            
                <ul class="sidebar-menu" id="MenuLists">
                                       
                </ul>

            </section>
            <!-- /.sidebar -->
        </aside>
        <!-- Content Wrapper. Contains page content -->
        <div class="content-wrapper">
            <!-- Content Header (Page header) -->
            <section class="content">
                <!-- Default box -->
                <div class="box">
                    <div id="loading" style="width: 100%; margin: auto; text-align: center; padding-top: 15%; padding-bottom: 12%; display: none;">
                        <img src="/common/bootstrap/img/asd.gif">
                    </div>
                    <div class="box-body" id="LoadData">

                        @RenderBody()
                        
                    </div>
                </div>
            </section>
        </div>

        <footer class="main-footer">
            <div class="pull-right hidden-xs">
                <b>Version</b> 1.0.0
            </div>
            <strong>Copyright &copy; 2016-2017.</strong>
        </footer>

      </div>       
</body>
</html>

